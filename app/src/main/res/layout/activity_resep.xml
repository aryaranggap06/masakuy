<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D5CED0"
    android:backgroundTint="#D5CED0">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="441dp"
        android:layout_height="245dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ellipse3" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057"
        app:srcCompat="@drawable/logo" />

    <ScrollView
        android:layout_width="336dp"
        android:layout_height="455dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/sateayam"
                android:layout_width="match_parent"
                android:layout_height="230dp"
                app:srcCompat="@drawable/fotosateayam" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:fontFamily="casual"
                android:gravity="center_horizontal|fill_vertical"
                android:text="Sate Ayam"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/nasigoreng"
                android:layout_width="match_parent"
                android:layout_height="230dp"
                app:srcCompat="@drawable/nasigoreng" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:fontFamily="casual"
                android:gravity="center_horizontal|fill_vertical"
                android:text="Nasi Goreng"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/sushi"
                android:layout_width="match_parent"
                android:layout_height="238dp"
                app:srcCompat="@drawable/fotosushi" />

            <TextView
                android:id="@+id/textsushi"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:fontFamily="casual"
                android:gravity="center_horizontal|fill_vertical"
                android:text="Sushi"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/burger"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fotoburger" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:fontFamily="casual"
                android:gravity="center_horizontal|fill_vertical"
                android:text="Burger"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/ramen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fotoramenpng" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:fontFamily="casual"
                android:gravity="center_horizontal|fill_vertical"
                android:text="Ramen"
                android:textSize="30sp" />
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>